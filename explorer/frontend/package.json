{
    "name": "@hlf-explorer/frontend",
    "version": "2.0.0",
    "license": "MIT",
    "author": "Renat Gubaev",
    "scripts": {
        "explorer:serve": "ng serve explorer",
        "explorer:build": "ng build explorer --configuration production",
        "explorer:version": "RELEASE=prerelease SOURCE=htdocs/explorer/config.json node $(node -e \"console.log(require.resolve('node-version-incrementer'))\") && git add htdocs/explorer/config.json",
        "start": "npm run explorer:serve",
        "build": "npm run explorer:build",
        "submodules:update": "git submodule init && git submodule update",
        "explorer:docker:image:build": "docker build -t hlf-explorer-frontend -f ./docker/explorer/Dockerfile .",
        "explorer:docker:image:remove": "docker rmi -f hlf-explorer-frontend:latest",
        "explorer:docker:container:run": "docker run -t -i -d -p 80:8080 -p 433:8443 -e PORT=8080 -e HTTP_PORT=8443 -e API_URL=http://localhost:3000 --name hlf-explorer-frontend hlf-explorer-frontend",
        "explorer:docker:container:start": "docker start hlf-tarot-frontend",
        "explorer:docker:container:stop": "docker stop hlf-tarot-frontend",
        "explorer:docker:container:remove": "docker rm hlf-tarot-frontend"
    },
    "pre-commit": [
        "explorer:version"
    ],
    "private": true,
    "dependencies": {
        "@angular/animations": "^13.2.2",
        "@angular/cdk": "^13.2.1",
        "@angular/cli": "^13.2.2",
        "@angular/common": "^13.2.2",
        "@angular/compiler": "^13.2.2",
        "@angular/core": "^13.2.2",
        "@angular/forms": "^13.2.2",
        "@angular/material": "^13.2.1",
        "@angular/material-moment-adapter": "^13.2.1",
        "@angular/platform-browser": "^13.2.2",
        "@angular/platform-browser-dynamic": "^13.2.2",
        "@angular/router": "^13.2.2",
        "@angular/service-worker": "^13.2.2",
        "@hlf-explorer/common": "^2.0.4",
        "@nguniversal/express-engine": "^13.0.2",
        "@ts-core/angular": "^13.0.9",
        "@ts-core/common": "^2.0.4",
        "@ts-core/frontend": "^2.0.1",
        "@types/numeral": "^2.0.2",
        "bootstrap": "^4.6.1",
        "classlist.js": "^1.1.20150312",
        "css-element-queries": "^1.2.3",
        "csshake": "^1.5.3",
        "express": "^4.17.2",
        "hammerjs": "^2.0.8",
        "highlight.js": "^11.4.0",
        "interactjs": "^1.10.11",
        "moment": "^2.29.1",
        "ngx-cookie": "^4.0.1",
        "numeral": "^2.0.6",
        "process": "^0.11.10",
        "rxjs": "^7.5.4",
        "tslib": "^2.3.1",
        "util": "^0.12.4",
        "zone.js": "^0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.1301.4",
        "@angular-devkit/build-angular": "^13.2.2",
        "@angular/compiler-cli": "^13.2.2",
        "node-version-incrementer": "^1.0.6",
        "pre-commit": "^1.2.2",
        "replace-in-file": "^6.3.2",
        "typescript": "^4.5.5"
    }
}
